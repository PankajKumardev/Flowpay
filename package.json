{
  "name": "week-18",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build": "npm run db:generate && turbo build",
    "db:generate": "cd packages/db && npx prisma generate --schema=packages/db/prisma/schema.prisma",
    "db:migrate": "cd packages/db && npx prisma migrate deploy --schema=packages/db/prisma/schema.prisma",
    "start-user-app": "cd ./apps/user-app && npm run start"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "prettier": "^3.2.5",
    "turbo": "^2.3.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "zod": "^3.23.8"
  }
}
